<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>UserStatisticsServlet</servlet-name>
        <servlet-class>cn.xkServlet.UserStatisticsServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>VisitServlet</servlet-name>
        <servlet-class>cn.xkServlet.VisitServlet</servlet-class>
    </servlet>



    <servlet-mapping>
        <servlet-name>UserStatisticsServlet</servlet-name>
        <url-pattern>/UserDistribution</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserStatisticsServlet</servlet-name>
        <url-pattern>/OSStatistics</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserStatisticsServlet</servlet-name>
        <url-pattern>/activityLevel</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>VisitServlet</servlet-name>
        <url-pattern>/HourVisit</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>VisitServlet</servlet-name>
        <url-pattern>/DayVisit</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>VisitServlet</servlet-name>
        <url-pattern>/MonthVisit</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>VisitServlet</servlet-name>
        <url-pattern>/YearVisit</url-pattern>
    </servlet-mapping>

</web-app>

